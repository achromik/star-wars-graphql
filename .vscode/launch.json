{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "name": "Serverless Debug",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "debug"],
      "sourceMaps": true,
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ]
    }
  ]
}
